<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>


  <!-- Versioning -->
    <Version>1.0.2-build01</Version>
    <AssemblyVersion>1.0.0.2</AssemblyVersion>
    <FileVersion>1.0.0.2</FileVersion>
    <PackageVersion>1.0.2</PackageVersion>

    <!-- NuGet Package Metadata -->
    <PackageId>OptiCore</PackageId>
    <Title>OptiCore</Title>
    <Description>OptiCore is a nugget to solve linear problems with the simplex method</Description>
    <Authors>Gaston Savino</Authors>
    <Company>RoosLLC</Company>
    <PackageProjectUrl>https://github.com/roosllc/opticore</PackageProjectUrl>
    <RepositoryUrl>https://github.com/roosllc/opticore</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <Copyright>Copyright © 2025 Gaston Savino</Copyright>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
  </PropertyGroup>


</Project>
